{
  "src/*.rs": {
    "alternate": "src/{}.rs",
    "type": "source"
  },
  "src/**/*.rs": {
    "alternate": "src/{}.rs",
    "type": "source"
  },
  "src/cli/*.rs": {
    "type": "command"
  },
  "src/db/*.rs": {
    "type": "db"
  },
  "src/models/*.rs": {
    "type": "model"
  },
  "src/context/*.rs": {
    "type": "context"
  },
  "src/integrity/*.rs": {
    "type": "integrity"
  },
  "src/tools/*.rs": {
    "type": "tool"
  }
}
